<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>Michael Collins on GitHub</title>
    <description>Ideas on software development and hobby and professional projects that I am working on.</description>
    <link>http://mfcollins3.github.com/index.html</link>
    <copyright>Copyright 2012 Michael F. Collins, III</copyright>
    <language>en-us</language>
    <pubDate>{{site.time | date:'%a, %d %b %Y %H:%M:%S %Z'}}</pubDate>
    {% for post in site.posts limit:20 %}
      <item>
        <title>{{post.title}}</title>
        <link>http://mfcollins3.github.com{{post.url}}</link>
        <description>{{post.description}}</description>
        <pubDate>{{post.date | date:'%a, %d %b %Y}}</pubDate>
      </item>
    {% endfor %}
  </channel>
</rss>